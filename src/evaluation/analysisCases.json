{
  "testCases": [
    {
      "request": "Show me a clock that displays hours and minutes",
      "expectedIntent": "new",
      "expectedUrl": "display/clock/digital/light",
      "expectedParams": {
        "format": "HH:mm",
        "size": 48
      }
    },
    {
      "request": "Make the clock show seconds too",
      "history": [
        {
          "request": "Show me a clock that displays hours and minutes",
          "params": {
            "format": "HH:mm",
            "size": 48
          }
        }
      ],
      "expectedIntent": "modify",
      "expectedUrl": "display/clock/digital/light",
      "expectedParams": {
        "format": "HH:mm:ss",
        "size": 48
      }
    },
    {
      "request": "Create a text display showing Hello World",
      "expectedIntent": "new",
      "expectedUrl": "display/text/card/light",
      "expectedParams": {
        "text": "Hello, World!",
        "size": 24
      }
    },
    {
      "request": "Make the text bigger",
      "history": [
        {
          "request": "Create a text display showing Hello World",
          "params": {
            "text": "Hello, World!",
            "size": 24
          }
        }
      ],
      "expectedIntent": "modify",
      "expectedUrl": "display/text/card/light",
      "expectedParams": {
        "text": "Hello, World!",
        "size": 36
      }
    }
  ]
}
